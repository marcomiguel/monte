<?php foreach (\Rpp\Services\Get\Content::part($this->model->nid,'contenido') as $key => $contenido): ?>                                  
    {% if contenido['tipo'] == "text" %}
     <p> {{contenido['texto']}} </p>
    <section class="related-news">
                          <ul>
                            <li>
                              <h4>{{contenido['cita']['titulo']}}</h4>
                              <h2><a href="#">{{contenido['cita']['desarrollo']}}</a></h2>
                            </li>
                          </ul>
    </section>
    {% elseif contenido['tipo'] == "photo" %} 
    <figure class="media full">
    <p> {{contenido['subtitulo']}} </p>
    {{ partial('partials/nota/contenido/bloque/'~model.package~'imagen',contenido) }}
    <figcaption class="help-media">{{contenido['texto']}}</figcaption>
    </figure>
    {% elseif contenido['tipo'] == "embed" %}
     <p> {{contenido['subtitulo']}} </p>
     <p> {{contenido['embed']}} <div>Credito: {{contenido['credito']}} | Via: {{contenido['via']}}</div></p>
     <p> {{contenido['texto']}} </p>
    {% elseif contenido['tipo'] == "youtube" %}  
    <p> {{contenido['subtitulo']}} </p>
<p>
<div id="swfplayer_{{contenido['youtube']['id']}}" class="nd_galeriaopen_{{contenido['youtube']['id']}}"></div></p>
<script type="text/javascript">setJW7("swfplayer_{{contenido['youtube']['id']}}", "https://www.youtube.com/watch?v={{contenido['youtube']['id']}}", "http://img.youtube.com/vi/{{contenido['youtube']['id']}}/0.jpg");</script>
<div>Credito: {{contenido['credito']}} | Via: {{contenido['via']}}</div>
{% elseif contenido['tipo'] == "video" %}

 <?php
